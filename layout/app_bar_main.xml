<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:a="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    a:layout_width="match_parent"
    a:layout_height="match_parent"
    tools:context="wo.main.MainActivity">

    <LinearLayout
        a:id="@+id/linearLayout"
        a:layout_width="match_parent"
        a:layout_height="match_parent"
        a:orientation="vertical">

        <android.support.design.widget.AppBarLayout
            a:layout_width="match_parent"
            a:layout_height="wrap_content"
            a:theme="@style/AppTheme.AppBarOverlay">

            <android.support.v7.widget.Toolbar
                a:id="@+id/toolbar"
                a:layout_width="match_parent"
                a:layout_height="?attr/actionBarSize"
                a:background="?attr/colorPrimary"
                app:popupTheme="@style/AppTheme.PopupOverlay" />

        </android.support.design.widget.AppBarLayout>

        <LinearLayout
            a:id="@+id/main"
            a:layout_width="match_parent"
            a:layout_height="match_parent"
            a:orientation="vertical"/>

        <LinearLayout
            a:id="@+id/interest"
            a:layout_width="match_parent"
            a:layout_height="match_parent"
            a:orientation="vertical" />

        <LinearLayout
            a:id="@+id/account"
            a:layout_width="match_parent"
            a:layout_height="match_parent"
            a:orientation="vertical" />

        <!--LinearLayout
            a:id="@+id/stock"
            a:layout_width="match_parent"
            a:layout_height="match_parent"
            a:orientation="vertical" /-->

        <LinearLayout
            a:id="@+id/info"
            a:layout_width="match_parent"
            a:layout_height="match_parent"
            a:orientation="vertical"/>


        <!--<include layout="@layout/content_main" />-->
    </LinearLayout>

    <android.support.design.widget.FloatingActionButton
        a:id="@+id/fab"
        a:layout_width="wrap_content"
        a:layout_height="wrap_content"
        a:layout_gravity="bottom|end"
        a:layout_margin="@dimen/fab_margin"
        a:backgroundTint="@color/colorPrimaryDark"
        a:src="@mipmap/alarm" />

</android.support.design.widget.CoordinatorLayout>
